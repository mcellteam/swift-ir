QTabBar::tab {
    font-size: 11px;
    height: 13px;
    min-width: 80px;
    max-width: 200;
    margin-bottom: 0px;
    padding-bottom: 0px;

}

QTabBar::tab:selected
{
    font-weight: 600;
}


QTabWidget::pane {
    border: 1px solid #161c20;
    background: #ede9e8;
}

QMenu {
    font-family: Tahoma, sans-serif;
	border-width: 1px;
	border-color: #1b1e23;
	border-style: outset;
	font-size: 10px;
}

/* affects main menu top-of-window*/
QMenuBar::item {
  	spacing: 4px;
	padding: 2px 2px;
  	background: transparent;
  	font-size: 10px;
}

QMenuBar::item:selected {
  	background: #969696;
}

QMenuBar {
	background: #1b2328;
	color: #f3f6fb;
	font-size: 10px;

}

QMenu::item {
  	height: 8px;
}

