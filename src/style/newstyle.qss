QMenu {
	border-width: 1px;
	border-color: #1b1e23;
	border-style: outset;
	font-size: 10px;
}

/* affects main menu top-of-window*/
QMenuBar::item {
  	spacing: 4px;
	padding: 2px 2px;
  	background: transparent;
  	font-size: 10px;
}

QMenuBar::item:selected {
  	background: #969696;
}

QMenuBar {
	background: #1b2328;
	color: #f3f6fb;
	font-size: 10px;

}

QMenu::item {
  	height: 8px;
}


QMenuBar {
background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 lightgray, stop:1 darkgray);
}

QMenuBar::item {
spacing: 3px; /* spacing between menu bar items */
padding: 1px 4px;
background: transparent;
border-radius: 4px;
}

QMenuBar::item:selected { /* when selected using mouse or keyboard */
background: #a8a8a8;
}

QMenuBar::item:pressed {
background: #888888;
}

/* Menus - context items and menubar items /
QMenu {
background-color: rgb(149, 166, 174); / grau */
}

QMenu::item {
background-color: transparent;
}

QMenu::item:selected {
background-color: rgb(163, 3, 47); /* rot */
}





